{
  "cSpellExt.enableDictionaries": ["spanish"],
  "sql.database": "postgresql",
  "cSpell.language": "en,es",
  "languageserver": {
    "zls": {
      "command": "/home/fraco/Workdir/zls/zig-out/bin/zls",
      "filetypes": ["zig"],
      "rootPatterns": ["build.zig", "zig.build"]
    },
    "astro-ls": {
      "command": "astro-ls",
      "args": ["--stdio"],
      "filetypes": ["astro"],
      "rootPatterns": [
        "package.json",
        "tsconfig.json",
        "jsconfig.json",
        ".git"
      ],
      "initializationOptions": {
        "typescript": {
          "tsdk": "node_modules/typescript/lib"
        }
      }
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  },
  "coc.preferences.formatOnSave": true,
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "snippets.ultisnips.pythonPrompt": false,
  "cSpell.userWords": [
    "Conv",
    "Farías",
    "ICPC",
    "Natrix",
    "SSSP",
    "USACO",
    "UTALCA",
    "UTFSM",
    "abcdefghijklmnñopqrstuvwxyz",
    "ayudantías",
    "Wronskiano",
    "bprint",
    "ccls",
    "crossterm",
    "cses",
    "cuda",
    "dsep",
    "dtype",
    "grayscale",
    "hmac",
    "imdb",
    "inplace",
    "objc",
    "objcpp",
    "optim",
    "pygame",
    "reqwest",
    "stoi",
    "symm",
    "ultisnips",
    "unsqueeze",
    "wineq"
  ]
}
